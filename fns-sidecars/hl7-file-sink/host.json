{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "eventHubs": {
      "batchCheckpointFrequency": 1,
      "maxEventBatchSize": 64,
      "prefetchCount": 128,
      "initialOffsetOptions": {
        "type": "fromStart",
        "enqueuedTimeUtc": ""
      }
    }
  },
  "logging": {
    "fileLoggingMode": "always",
     "logLevel": {
      "default": "Warning",
      "Function.storage-sink": "Information",
      "Host": "Error",
      "Worker": "Warning",
      "Microsoft": "None"
    }
  }

}
